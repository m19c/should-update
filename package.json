{
  "name": "should-update",
  "version": "2.0.0",
  "description": "A shouldComponentUpdate-wrapper",
  "main": "build/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint index.js test/",
    "build": "babel -d build/ index.js",
    "travis": "npm run test && npm run lint"
  },
  "keywords": [
    "react",
    "component",
    "shouldComponentUpdate",
    "should",
    "update"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrBoolean/should-update.git"
  },
  "bugs": {
    "url": "https://github.com/MrBoolean/should-update/issues"
  },
  "author": "Marc Binder <marcandrebinder@gmail.com> (MrBoolean)",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-airbnb": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "jsdom": "^16.5.0"
  },
  "dependencies": {
    "deep-property": "^1.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/test/**",
      "!**/node_modules/**"
    ],
    "testMatch": [
      "<rootDir>/**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "coverageDirectory": "<rootDir>/reports/coverage"
  }
}
